{
  "manifest_version": 2,
  "name": "DuckDuckGoGoogleBangCounter",
  "version": "1.0",
  "description": "How many Google Bangs are you using?",
  "homepage_url": "https://github.com/orange-puff/duckduckgo-bang-counter",
  "icons": {
    "96": "icons/duckduckgo-google-bang-counter-96.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icons/duckduckgo-google-bang-counter-96.png",
    "default_title": "DuckDuckGo Google Bang Counter",
    "default_popup": "popup/duckduckgo-google-bang-counter.html"
  },
  "background": {
    "page": "background/background-page.html"
  }
}